generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Products {
  id         String   @id @default(uuid())
  image      String
  brand      String
  price      String
  name       String
  created_at DateTime @default(now())

  @@map("products")
}
